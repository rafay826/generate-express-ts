  "files": [
    "dist"
  ],
  "dependencies": {
    "shelljs": "^0.8.5",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.4",
    "@types/shelljs": "^0.8.15"
  },
  "engines": {
    "node": ">=16"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
